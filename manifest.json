
{
  "manifest_version": 2,
  "name": "Foxmote",
  "description": "Yet another XBMC remote, compatible frodo and above.",
  "version": "1.1.2",
  "app": {
    "background": {
      "scripts": ["js/chrome.js"]
    }
  },
  "permissions": ["storage","<all_urls>"],
  "icons": {
    "16": "img/icons/foxmote.png",
    "32": "img/icons/foxmote-32.png",
    "64": "img/icons/foxmote-64.png",
    "128": "img/icons/foxmote-128.png"
  }
}